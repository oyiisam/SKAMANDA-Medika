<IfModule mod_rewrite.c>
    RewriteEngine On
    # RewriteBase /

# Mengecualikan akses untuk folder yang terlarang

# Mengecualikan akses untuk file CSS, JavaScript, gambar, dll

# Aturan untuk memberikan status Forbidden
RewriteCond %{REQUEST_URI} !^/(assets|sql|upload|vendor)/
RewriteCond %{REQUEST_URI} !\.(jpg|jpeg|png|gif|css|js|woff|woff2|ttf|otf|eot|svg|ico|xlsx|xls|mp3)$
RewriteRule ^(assets|sql|upload|vendor)/ - [F,L]

 
   # Remove index.php from URL
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ index.php/$1 [L]

RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]


</IfModule>

